{
    "name": "Rbac Transfer",
    "version": "2.0.0",
    "manifest_version": 2,
    "description": "Rbac Transfer",
    "browser_action": {
        "default_title": "Rbac Transfer"
    },
    "content_scripts": [{
        "matches": ["http://*.wormpex.com/html/rbac/auth*"],
        "js": ["content.js"],
        "css": ["content.css"],
        "run_at": "document_end"
    }],
    "background": {
        "persistent": true,
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "activeTab",
        "background",
        "cookies",
        "unlimitedStorage",
        "notifications"
    ]
}